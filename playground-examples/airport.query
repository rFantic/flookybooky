query getAirports {
  airport {
    id
    name
    address
  }
}

mutation createAirport {
  createAirport(
    input: {
      name: "Noi Bai International Airport (HAN)",
      address: "Phú Minh, Sóc Sơn, Hanoi",
    }
  ) {
    id
    name
    address
  }
}

mutation createAirport2 {
  createAirport(
    input: {
      name: "Bardan International Airport (HAN)",
      address: "Da Nang",
    }
  ) {
    id
    name
    address
  }
}

