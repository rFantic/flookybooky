query get_customers {
  customers {
    id
    name
    address
    email
    license_id
  }
}

mutation create_customers {
  createCustomer(
    input: {
      name: "near nguyen", 
      address: "fake address", 
      license_id: "034098021748", 
      phone_number: "0123456789"
      email: "near@gmail.com"
    }) {
    id
    name
    address
    email
    license_id
  }
}


